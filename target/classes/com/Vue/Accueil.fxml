<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.Modele.AccueilController">
    <children>
        <HBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" spacing="50.0">
            <Button fx:id="connexionButton" mnemonicParsing="false" text="Se connecter" onAction="#handleConnexionButton">
                <font>
                    <Font name="System Bold" size="14.0" />
                </font>
            </Button>
            <Button fx:id="inscriptionButton" mnemonicParsing="false" text="S'inscrire" onAction="#handleInscriptionButton">
                <font>
                    <Font name="System Bold" size="14.0" />
                </font>
            </Button>
        </HBox>
    </children>
</VBox>
